#!/bin/bash
BENCH=s713
HAPA=/home/frathoso/Dropbox/ASpring_2014/Capstone/atpg 
$HAPA/bin/atpg -D 1 -X R -b $HAPA/benchmarks/$BENCH.bench -u run/$BENCH -t run/$BENCH > $HAPA/run/results.txt

wine $HAPA/bin/atalanta.exe $HAPA/benchmarks/$BENCH.bench -l run/$BENCH.log -t run/$BENCH.test> $HAPA/run/ataresults.txt
